<Context path="/authorization" docBase="KheopsAuthorization.war">
    <Parameter name="online.kheops.superuser.hmacsecret" value="${kheops_superuser_hmasecret}"
               override="false"/>
    <Parameter name="online.kheops.auth.hmacsecret" value="${kheops_auth_hmasecret}"
               override="false"/>
    <Parameter name="online.kheops.pacs.uri" value="${kheops_pacs_url}"
               override="false"/>
    <Parameter name="online.kheops.jdbc.url" value="jdbc:${kheops_mysql_url}"
               override="false"/>
    <Parameter name="online.kheops.jdbc.user" value="${kheops_mysql_user}"
               override="false"/>
    <Parameter name="online.kheops.jdbc.password" value="${mysql_password}"
               override="false"/>
</Context>
